    "require": {
        "php": ">=5.5.9",
        "acquia/http-hmac-php": "^4.0",
        "webmozart/assert": "^1.4"
    },
    "require-dev": {
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "fzaninotto/faker": "~1.4",
        "orchestra/database": "3.3.*|3.4.*|3.5.*|3.6.*|3.7.*",
